{
  "name": "SullyGnome Helper",
  "description": "Easy linking to SullyGnome.com from Twitch",
  "version": "0.1.0",
  "permissions": [
	"http://www.twitch.tv/*",
	"https://www.twitch.tv/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
	"default_icon": "icon.png",
    "default_title": "Go to SullyGnome.com"
  },
  "web_accessible_resources": [
	"icon.png"
  ],
  "manifest_version": 2
}
