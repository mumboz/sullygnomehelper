{
  "name": "SullyGnome Helper",
  "description": "Easy linking to SullyGnome.com from Twitch",
  "version": "0.1.3",
  "icons": { 
    "16": "icon-16.png",
	"19": "icon-19.png",
    "48": "icon-48.png",
    "128": "icon-128.png" 
  },
  "permissions": [
	"activeTab",
	"http://www.twitch.tv/*",
	"https://www.twitch.tv/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
	"default_icon": "icon-19.png",
    "default_title": "SullyGnome Helper"
  },
  "web_accessible_resources": [
	"icon-19.png"
  ],
  "manifest_version": 2
}